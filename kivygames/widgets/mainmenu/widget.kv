#:kivy 1.11.1

<MainMenu>:
    padding: 20
    orientation: 'vertical'
    Label:
        size_hint: 1, None
        text: 'Kivy Games'
    GridLayout:
        cols: 3
        id: buttonGrid
    PlayerSelector:
        gameObject: root.gameLayout.gameObject if root.gameLayout else None
        on_choose: root.startGame(*args)